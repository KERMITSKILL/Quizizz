{
    "manifest_version": 3,
    "name": "Answers For Quizizz",
    "description": "Highlights the right answer for most quizizz questions",
    "version": "1.0",
    "permissions": [
        "tabs",
        "scripting"
    ],"host_permissions": [
        "https://*/*"
    ],
    "action": {
        "default_popup": "static/popup.html",
	"default_icon": {
            "128" : "ico.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },"icons" : {
        "128" : "ico.png"
    }
}